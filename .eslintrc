{
  "parser": "babel-eslint",
  "extends": "google",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [2, 80, 4, {"ignoreUrls": true, "ignoreComments": true}],
    "require-jsdoc": [1, {
      require: {
        FunctionDeclaration: true,
        MethodDefinition: true,
        ClassDeclaration: true
      }
    }]
  }
}